<h1>All Projects</h1>
<div class="row placeholders">
  <div class="center-container">
    <table class="table">
      <tbody class="tasks">
        <% Project.all.each do |project| %>
          <tr id=<%=project.id%> >
            <td><%= link_to project.name, project_path(project)%></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="right-container">
  </div>

</div>
