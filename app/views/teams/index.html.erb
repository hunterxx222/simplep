<div class="container" style="background:lightblue">
  <h2>Your Teams</h2>
    <%= link_to 'Create Team', new_team_path, remote: true, data: { toggle: "modal", target: "#ajax-modal-project" }, class: 'btn btn-outline-primary', style: "float:right" %><br><br>
  <table class="table table-hover table-striped">
    <thead>
      <th>
        Name
      </th>
      <th></th>
    </thead>
    <tbody class="teams">
      <% @teams.each do |team|%>
      <tr id=<%= team.id %> >
        <%= render 'team', team: team %>
      </tr>
      <% end %>
    </tbody>
  </table>

</div>
